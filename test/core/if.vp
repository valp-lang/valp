// WITH BRACKETS
var x = 10;
if (x == 10) {
  x = 11;
}
assert_equal(11, x);

x = 10;
if (x != 10) {
} else {
  x = 11;
}
assert_equal(11, x);

// WITHOUT BRACKETS
var x = 10;
if x == 10 {
  x = 11;
}
assert_equal(11, x);

x = 10;
if x != 10 {
} else {
  x = 11;
}
assert_equal(11, x);

// TERNARY OPERATOR
x = 0;

1 == 1; ? x = 1; : x = 2;
assert_equal(x, 1);

1 != 1; ? x = 1; : x = 2;
assert_equal(x, 2);
