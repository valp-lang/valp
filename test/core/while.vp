// WITH BRACKETS
var x = 0;
while (x != 1) {
  x = x + 1;
}
assert_equal(x, 1);

// WITHOUT BRACKETS
var x = 0;
while x != 1 {
  x = x + 1;
}
assert_equal(x, 1);

// WITH NEXT
var x = 0;
var res = 0;

while x != 3 {
  x += 1;
  if x == 2 { next; }
  // print res;
  res += 5;
}

assert_equal(res, 10);